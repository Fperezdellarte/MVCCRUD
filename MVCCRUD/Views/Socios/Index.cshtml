@model List<MVCCRUD.DTO.SocioDTO>

<h1 class="text-white">Socios</h1>
	<p>
	<a class="btn btn-info text-white" asp-action="Create">Crear nuevo producto</a>

	</p>
	<table class="table">
		<thead>
			<tr>
			<th class="text-white">Nombre</th>
			<th class="text-white">FechaIngreso</th>
			<th class="text-white">Dni</th>
			<th class="text-white">Opciones administrador</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in Model)
			{
				<tr>
				<td class="text-white">
						@Html.DisplayFor(modelItem=>item.Nombre)
					</td>
				<td class="text-white">
					@Html.DisplayFor(modelItem=>item.FechaIngreso)
				</td>
				<td class="text-white">
					@Html.DisplayFor(modelItem=>item.Dni)
				</td>
				<td>
					<a asp-action="Edit" class="btn btn-warning" asp-route-id="@item.Id">Editar</a>
					<a asp-action="Delete" class="btn btn-dark" asp-route-id="@item.Id">Eliminar</a>
				</td>
				</tr>
			}
		</tbody>
	</table>