@model MVCCRUD.DTO.ClaseDTO

<h1 class="text-white">Eliminar clase</h1>

<div>
	<h4 class="text-white">Clase</h4>
	<hr />
	<dl class="row">
		<dt class="col-sm-2 text-white">
			@Html.DisplayNameFor(model => model.Nombre)
		</dt>
		<dd class="col-sm-2 text-white">
			@Html.DisplayFor(model => model.Nombre)
		</dd>
		<dt class="col-sm-2 text-white">
			@Html.DisplayNameFor(model => model.Profesor)
		</dt>
		<dd class="col-sm-2 text-white">
			@Html.DisplayFor(model => model.Profesor)
		</dd>
		<dt class="col-sm-2 text-white">
			@Html.DisplayNameFor(model => model.DiaClase)
		</dt>
		<dd class="col-sm-2 text-white">
			@Html.DisplayFor(model => model.DiaClase)
		</dd>
	</dl>
	<form asp-action="Delete" asp-route-id="@Model.Id">
		<input type="submit" value="Delete" class="btn btn-danger">
		<a class="btn btn-info text-white" asp-action="Index">Regresar al Index</a>
	</form>
</div>
